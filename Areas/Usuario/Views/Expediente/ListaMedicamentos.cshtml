@using ProyectoProgramadoLenguajes2024.Models.ViewModels
@model MedicamentosPacientesVM
@{
    ViewData["Title"] = "Expediente - Medicamentos";
    var pacienteId = ViewData["PacienteId"];
}

<partial name="_Notifications" />

<div class="container">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">
                Medicamentos
            </h2>
        </div>
    </div>

    <input type="hidden" id="pacienteId" value="@pacienteId" />

    <table id="tblData" class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>Medicamento</th>
                <th>Recetado Por</th>
            </tr>
        </thead>
    </table>
</div>

<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/js/listaMedicamentoPaciente.js"></script>
